<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="276dp"
        android:layout_height="135dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_menu_logo"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Periodos de Inscripción"
        android:textColor="#0095C8"
        android:textSize="18sp"
        android:textStyle="bold"
        app:fontFamily="sans-serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <android.support.v7.widget.GridLayout
        android:id="@+id/gridLayout2"
        android:layout_width="392dp"
        android:layout_height="263dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.137"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="9dp">

        <TextView
            android:id="@+id/lblNombre"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:background="@color/colorPrimaryDark"
            android:text="Nombre"
            android:textColor="@color/colorPrimary"
            android:textSize="12sp"
            android:textStyle="bold"
            app:fontFamily="sans-serif"
            app:layout_column="1"
            app:layout_row="0" />

        <TextView
            android:id="@+id/lblDescripcion"
            android:layout_width="228dp"
            android:layout_height="wrap_content"
            android:background="@color/colorPrimaryDark"
            android:text="Descripción"
            android:textColor="@color/colorPrimary"
            android:textSize="12sp"
            android:textStyle="bold"
            app:fontFamily="sans-serif"
            app:layout_column="10"
            app:layout_row="0" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="140dp"
            android:layout_height="6dp"
            android:textSize="12sp"
            app:layout_column="1"
            app:layout_row="1" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="171dp"
            android:layout_height="6dp"
            android:textSize="12sp"
            app:layout_column="10"
            app:layout_row="1" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="140dp"
            android:layout_height="35dp"
            android:text="Primer periódo de inscripción cursos regulares"
            android:textSize="10sp"
            app:layout_column="1"
            app:layout_row="2" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="171dp"
            android:layout_height="34dp"
            android:text="Enero- Febrero 2019."
            android:textSize="10sp"
            app:layout_column="10"
            app:layout_row="2" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="140dp"
            android:layout_height="9dp"
            app:layout_column="1"
            app:layout_row="3" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="171dp"
            android:layout_height="9dp"
            app:layout_column="10"
            app:layout_row="3" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:text="Segundo periódo de inscripción cursos regulares"
            android:textSize="10sp"
            app:layout_column="1"
            app:layout_row="4" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="171dp"
            android:layout_height="29dp"
            android:text="Junio - Julio 2019."
            android:textSize="10sp"
            app:layout_column="10"
            app:layout_row="4" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="140dp"
            android:layout_height="7dp"
            app:layout_column="1"
            app:layout_row="5" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="171dp"
            android:layout_height="8dp"
            app:layout_column="10"
            app:layout_row="5" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:text="Requisitos de inscripción"
            android:textSize="10sp"
            app:layout_column="1"
            app:layout_row="6" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="171dp"
            android:layout_height="wrap_content"
            android:text="Copia de acta de nacimiento."
            android:textSize="10sp"
            app:layout_column="10"
            app:layout_row="6" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="171dp"
            android:layout_height="wrap_content"
            android:text="Copia de CURP."
            android:textSize="10sp"
            app:layout_column="10"
            app:layout_row="7" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="171dp"
            android:layout_height="wrap_content"
            android:text="Copia de comprobante de domicilio"
            android:textSize="10sp"
            app:layout_column="10"
            app:layout_row="8" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="171dp"
            android:layout_height="wrap_content"
            android:text="Copia de identificación oficial con fotografía."
            android:textSize="10sp"
            app:layout_column="10"
            app:layout_row="9" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="171dp"
            android:layout_height="wrap_content"
            android:text="Foto tamaño infantil."
            android:textSize="10sp"
            app:layout_column="10"
            app:layout_row="10" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="171dp"
            android:layout_height="wrap_content"
            android:text="Efectuar pago único por semestre."
            android:textSize="10sp"
            app:layout_column="10"
            app:layout_row="11" />

    </android.support.v7.widget.GridLayout>

</android.support.constraint.ConstraintLayout>
